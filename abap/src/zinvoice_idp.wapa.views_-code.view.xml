<mvc:View                                                                                                                                                                                                                                                      
    controllerName="aws.fin.ap.controllers.Code"                                                                                                                                                                                                               
    xmlns:mvc="sap.ui.core.mvc"                                                                                                                                                                                                                                
    xmlns:ce="sap.ui.codeeditor"                                                                                                                                                                                                                               
    xmlns="sap.m">                                                                                                                                                                                                                                             
    <Page showNavButton="true"                                                                                                                                                                                                                                 
    title="AWS SDK for ABAP code samples"                                                                                                                                                                                                                      
    titleLevel="H3"                                                                                                                                                                                                                                            
          navButtonPress="onPageCodeNavButtonPress">                                                                                                                                                                                                           
        <IconTabHeader backgroundDesign="Translucent"                                                                                                                                                                                                          
                       select="onIconTabHeaderSelect"                                                                                                                                                                                                          
                       selectedKey="S3">                                                                                                                                                                                                                       
            <items>                                                                                                                                                                                                                                            
                <IconTabFilter key="S3" text="Amazon S3" >                                                                                                                                                                                                     
                </IconTabFilter>                                                                                                                                                                                                                               
                <IconTabFilter key="Translate" text="Amazon Translate" />                                                                                                                                                                                      
                <IconTabFilter key="Textract" text="Amazon Textract" />                                                                                                                                                                                        
                <IconTabFilter key="Lambda" text="AWS Lambda"/>                                                                                                                                                                                                
                <IconTabFilter key="Bedrock" text="Amazon Bedrock"/>                                                                                                                                                                                           
            </items>                                                                                                                                                                                                                                           
        </IconTabHeader>                                                                                                                                                                                                                                       
        <ce:CodeEditor id="idCodeEditor"&nbsp;                                                                                                                                                                                                                 
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                 
                       type="abap"&nbsp;                                                                                                                                                                                                                       
                       editable="false"&nbsp;                                                                                                                                                                                                                  
                       colorTheme="hcb_bright"/>                                                                                                                                                                                                               
                                                                                                                                                                                                                                                               
     <footer>                                                                                                                                                                                                                                                  
        <Toolbar ></Toolbar>                                                                                                                                                                                                                                   
     </footer>                                                                                                                                                                                                                                                 
    </Page>                                                                                                                                                                                                                                                    
                                                                                                                                                                                                                                                               
</mvc:View>                                                                                                                                                                                                                                                    